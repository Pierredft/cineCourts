<form method="post" action="{{ path('app_video_delete', {'id': video.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimé cette video ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ video.id) }}">
    <div class="posBtn">
        <button class="btn btnRegDanger" style="width: 40%;">Delete</button>
    </div>
</form>
